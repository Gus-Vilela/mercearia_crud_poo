            <TableColumn fx:id="salarioCol" prefWidth="284.0" styleClass="colum" text="Salário Base R$" />
            <TableColumn fx:id="comissaoCol" prefWidth="204.0" styleClass="colum" text=" Comissão %" />
        </columns>
      </TableView>
      <Text fx:id="campoObri" fill="#c30b0b" layoutX="681.0" layoutY="366.0" opacity="0.0" strokeType="OUTSIDE" strokeWidth="0.0" text="* Campo Obrigatório" />
